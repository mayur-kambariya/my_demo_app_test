<% if current_admin %>
  <%= link_to 'Cleaners', cleaners_path %>
  <%= link_to 'logout', destroy_admin_session_path,method: :delete %>
<% else %>
  <% if session[:customer].nil? %>
    <%= link_to 'Login_Customer', new_customer_path,method: :get %>
    <%= link_to 'Login_Admin', new_admin_session_path,method: :get %>
  <% else %>
  <%= link_to 'Booking', new_booking_path,method: :get %>
  <%= button_to 'Logout',logout_customer_path, method: :post  %>
  <% end %>
<% end %>
